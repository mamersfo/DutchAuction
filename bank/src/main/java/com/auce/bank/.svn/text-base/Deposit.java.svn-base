package com.auce.bank;

import java.util.Date;

public class Deposit extends Mutation
{
	public Deposit(
		Date date,
		Account account,
		String description,
		long amount )
	{
		super( date, 
			account.getAccountNumber(),
			account.getNameOfHolder(), 
			account.getCityOfHolder(), 
			description, amount );
	}
	
	public Deposit( 
		Date date, String accountNumber, 
		String nameOfHolder, String cityOfHolder,
		String description, long amount )
	{
		super( date, accountNumber, nameOfHolder, cityOfHolder, description, amount );
	}
}
